{
    "name": "deegitalbe/trustup-versioned-package",
    "description": "Synchronization package checking that our packages are using same version everywhere.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-api-client": "^1.0",
        "henrotaym/laravel-test-suite": "^2.0",
        "deegitalbe/server-authorization": "^1.1",
        "henrotaym/laravel-helpers": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Deegitalbe\\TrustupVersionedPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deegitalbe\\TrustupVersionedPackage\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deegitalbe\\TrustupVersionedPackage\\Providers\\TrustupVersionedPackageServiceProvider"
            ]
        }
    }
}
